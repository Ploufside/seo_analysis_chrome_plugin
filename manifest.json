{
    "name": "Seo Extension",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Opens several SEO analysis tools for the current webpage",
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Seo Extension",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "tabs"
    ]
}